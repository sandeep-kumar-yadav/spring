<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="dojDT" class="java.util.Date">
<property name="year" value="119"></property>
<property name="month" value="1"></property>
<property name="date" value="28"></property>

</bean>
<bean id="aadhar" class="com.nt.bean.User">
  <constructor-arg value="123456"></constructor-arg>
  <constructor-arg value="sandeep"></constructor-arg>
    <constructor-arg ref="dojDT"></constructor-arg>
  
</bean>
</beans>


